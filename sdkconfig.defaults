CONFIG_ESP32S2_INSTRUCTION_CACHE_16KB=y
CONFIG_ESP32S2_INSTRUCTION_CACHE_LINE_16B=y
CONFIG_ESP32S2_DATA_CACHE_8KB=y
CONFIG_ESP32S2_DATA_CACHE_LINE_32B=y

